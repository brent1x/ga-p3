      </div>
      <h2 class="border-line">Your Queues</h2>
      <div class="text-left">
      <% @cues.each do |x| %>

      <% if x.user_id == session[:user_id] %>
        <div class="row">
          <!-- For loop for Q goes here -->
          <div class="col-xs-12 text-center">
            <!-- Queue Name -->
            <h4><%= x.name %> </h4>
          </div>
        </div>
        <div class="row">
          <!-- For loop for Q goes here -->
          <div class="col-xs-6">
            <!-- Restaurant date -->
            <h4><%= x.start_date.strftime('%a %d %b') %> </h4>
            <!-- q time -->
            <h4><%= x.start_time.strftime('%I:%M:%S %p') %> - <%= x.end_time.strftime('%I:%M:%S %p') %></h4>
            <h4 class="party-padding">Party Size: <%= x.covers %></h4>
          </div>
          <div class="col-xs-6 text-right">
            <span class="not-selected-item delete-width"><%= link_to "Delete", queue_path(x.id), method:"Delete" %></span>
          </div>
        </div>Â 
         <!-- End Loop -->
      <div class="end-line"></div>
      <p class="not-selected-item text-center expand-width">
        <%= link_to "Add Restaurants to #{x.name}", cue_path(x.id) %>

      </p>
        <% end %>
        <% end %>
      </div>
      </div>
