<div id="crimson">
  <div class="overlow-hidden">
    <div class="container text-center text-white">
      <div >
        <img class="logo-image-sm" src="<%= asset_path('rezqlogo.png') %>" alt="">
      </div>
      <h2 class="border-line">Your Queue</h2>
      <div class="text-left">
      <% @cues.each do |x| %>

      <% if x.user_id == session[:user_id] %>
        <div class="row">
          <!-- For loop for Q goes here -->
          <div class="col-xs-6">
            <!-- Restaurant date -->
            <h4><%= x.start_date.strftime('%a %d %b') %> - <%= x.start_date.strftime('%a %d %b') %></h4>
            <!-- restaurant name -->
            <h3><%= x.rests %></h3>
            <!-- q time -->
            <h4><%= x.start_time.strftime('%I:%M:%S %p') %> - <%= x.end_time.strftime('%I:%M:%S %p') %></h4>
          </div>
          <div class="col-xs-6 text-right">
            <span class="not-selected-item delete-width"><%= link_to "Delete", queue_path(x.id), method:"Delete" %></span>
            <h4 class="party-padding">Party Size: <%= x.covers %></h4>
          </div>

        </div>
        <% end %>
        <% end %>
      </div>


      <!-- End Loop -->
      <div class="end-line"></div>
      <p class="not-selected-item text-center expand-width">
        <%= link_to "Add Restaurant to Queue", new_queue_path %>

      </p>
    </div>
    <br><br> <br><br><br><br><br><br><br>
  </div>
</div>

